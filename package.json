{
    "name": "plugins",
    "private": true,
    "license": "MIT",
    "workspaces": [
        "starters/*",
        "plugins/*"
    ],
    "scripts": {
        "clean": "rm -rf ./p",
        "copy": "bash ./scripts/copy-build.sh",
        "dev": "turbo run dev --concurrency=20",
        "build": "turbo run build --env-mode=loose && run copy",
        "typecheck": "turbo run typecheck"
    },
    "devDependencies": {
        "turbo": "^2.1.2"
    },
    "packageManager": "yarn@4.9.1"
}
