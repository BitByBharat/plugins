{
  "name": "renamer",
  "private": true,
  "version": "0.0.0",
  "type": "module",
  "scripts": {
    "check": "svelte-check --tsconfig ./tsconfig.json",
    "dev": "vite",
    "format": "prettier --check '**/*.{js,json,jsx,md,ts,svelte,css}' --write",
    "build": "vite build",
    "check-eslint": "run g:check-eslint",
    "pack": "npx framer-plugin-tools@latest pack",
    "preview": "vite preview",
    "check-typescript": "run g:check-typescript"
  },
  "dependencies": {
    "framer-plugin": "^3.3.2"
  },
  "devDependencies": {
    "@sveltejs/vite-plugin-svelte": "^5.1.0",
    "prettier": "^3.3.3",
    "prettier-plugin-svelte": "^3.2.6",
    "svelte": "^5.0.0-next.201",
    "svelte-check": "^3.8.4",
    "tslib": "^2.6.3",
    "typescript": "^5.8.3",
    "vite": "^6.3.5",
    "vite-plugin-framer": "^1.0.7",
    "vite-plugin-mkcert": "^1"
  }
}
